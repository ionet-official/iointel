#!/bin/bash

COMPOSE_DOCKER_CLI_BUILD=1 \
DOCKER_BUILDKIT=1 \
TUNNEL_PASS=`cat creds.env | grep "TUNNEL_PASS" | cut -d "=" -f2` \
docker compose -f framework/infra/compose/docker-compose.yml \
               -f framework/infra/compose/owui-compose.yml \
               -f framework/infra/compose/flowise-compose.yml $@

